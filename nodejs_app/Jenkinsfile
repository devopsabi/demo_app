pipeline {
        agent any
        
        stages {
        
            stage('checkout_nodejs_app') {
            when {expression { params.project_name == 'nodejs' }}
                steps {
                    git branch:"${BRANCH}", url: 'https://github.com/devopsabi/demo_app.git', credentialsId:'git_hub_id'
                    sh "ls -lat"
                }
    
            }
    }
}
